import re

inputfile = 'C:\\Users\\PeterH\\eclipsemars\\TrainingResources\\documents\\X00testdocs\\regex_sum_42.txt'

regexfile = open(inputfile).read()
print regexfile
 
for line in regexfile:
    numberlist = re.findall('[0-9]+', line)
 
print numberlist



