isbn_kw_dict = {}

inputfile = "C://Users//PeterH//eclipsemars//VLBGold//documents//X01inputdocs//VLBReport_VuR_ohneSchlagworte_LIST_525.txt"
sourcefile = "C://Users//PeterH//eclipsemars//VLBGold//documents//X01inputdocs//dictDNB_9783525complete.txt"
outputfile = "C://Users//PeterH//eclipsemars//VLBGold//documents//X03finalresults//Schlagworte525.csv"
finp = open(inputfile)        
finpeval = eval(finp.read())

fsrc = open(sourcefile)        
fsrceval = eval(fsrc.read())

for isbn in finpeval:
    try:
        isbn_kw_dict[isbn] = fsrceval[isbn][8]
        #print isbn, fsrceval[isbn][8]
    except:
        isbn_kw_dict[isbn] = []
        #print isbn, "nicht vorhanden"

print isbn_kw_dict


        
