import pdfrw
import PyPDF2

mining = open('F:\eb_9783737004541_052812.pdf', 'r')

kwindex = dict()

for word in mining: 
    if word not in kwindex:
        counter = 0
        kwindex[word] = counter + 1
    else:
        kwindex[word] = counter + 1

print kwindex




